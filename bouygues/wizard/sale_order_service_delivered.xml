<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- wizard view -->
        <record id="bouygues_sale_order_service_delivered_view_form" model="ir.ui.view">
            <field name="name">Add delivered quantity</field>
            <field name="model">sale.order.service.delivered</field>
            <field name="arch" type="xml">
                <form string="Add delivered quantity">
                    <group>
                        <field name="qty_delivered"/>
                    </group>
                    <footer>
                        <button string="Apply" name="action_apply" type="object" class="btn-primary"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>
