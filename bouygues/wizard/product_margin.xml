<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- wizard view -->
        <record id="bouygues_product_margin_view_form" model="ir.ui.view">
            <field name="name">Product Margin</field>
            <field name="model">product.margin</field>
            <field name="arch" type="xml">
                <form string="Product Margin">
                    <group>
                        <field name="start_date"/>
                        <field name="end_date"/>
                    </group>
                    <footer>
                        <button string="Apply" name="action_apply" type="object" class="btn-primary"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>
