<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- wizard view -->
        <record id="bouygues_inventory_extraction_date_view_form" model="ir.ui.view">
            <field name="name">Inventory Extraction Date</field>
            <field name="model">inventory.extraction.date</field>
            <field name="arch" type="xml">
                <form string="Inventory Date">
                    <group>
                        <field name="inventory_date"/>
                    </group>
                    <footer>
                        <button string="Apply" name="action_apply" type="object" class="btn-primary"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>
