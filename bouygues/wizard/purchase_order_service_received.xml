<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- wizard view -->
        <record id="bouygues_purchase_order_service_received_view_form" model="ir.ui.view">
            <field name="name">Add received quantity</field>
            <field name="model">purchase.order.service.received</field>
            <field name="arch" type="xml">
                <form string="Add received quantity">
                    <group>
                        <field name="qty_received"/>
                    </group>
                    <footer>
                        <button string="Apply" name="action_apply" type="object" class="btn-primary"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>
