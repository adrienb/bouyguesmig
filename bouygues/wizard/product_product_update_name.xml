<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- wizard view -->
        <record id="bouygues_product_product_update_name_view_form" model="ir.ui.view">
            <field name="name">Update Name</field>
            <field name="model">product.product.update.name</field>
            <field name="arch" type="xml">
                <form string="Update Name">
                    <group>
                        <field name="name"/>
                    </group>
                    <footer>
                        <button string="Apply" name="action_apply" type="object" class="btn-primary"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>
