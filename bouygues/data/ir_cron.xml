<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_intersite_printed" model="ir.cron">
            <field name="name">Mark today intersite pickings as printed</field>
            <field name="model_id" ref="stock.model_stock_picking"/>
            <field name="state">code</field>
            <field name="code">model._mark_pickings_printed()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="nextcall" eval="(datetime.now().replace(hour=0, minute=0, second=0)+timedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>
        <record id="ir_cron_reception_to_process" model="ir.cron">
            <field name="name">Send mail: Receptions to process</field>
            <field name="model_id" ref="stock.model_stock_picking"/>
            <field name="state">code</field>
            <field name="code">model._send_mail_reception_to_process()</field>
            <field name="nextcall" eval="(datetime.now().replace(hour=8, minute=0, second=0)+timedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>
        <record id="ir_cron_archive_product_product" model="ir.cron">
            <field name="name">Archive end of life product.product with 0 qty</field>
            <field name="model_id" ref="product.model_product_product"/>
            <field name="state">code</field>
            <field name="code">model._archive_end_of_life()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="nextcall" eval="(datetime.now().replace(hour=0, minute=0, second=0)+timedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>
        <record id="ir_cron_recompute_late_dropships_days_delivery_today" model="ir.cron">
            <field name="name">Recompute number_of_late_dropships and days_between_delivery_today every day for the dashboard</field>
            <field name="model_id" ref="purchase.model_purchase_order"/>
            <field name="state">code</field>
            <field name="code">model._recompute_late_dropships_days_delivery_today()</field>
            <field name="nextcall" eval="(datetime.now().replace(hour=0, minute=0, second=0)+timedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>
    </data>
</odoo>
