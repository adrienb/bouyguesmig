<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="archive_assets_frontend" name="archive assets" inherit_id="web.assets_frontend">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" href="/bouygues/static/src/css/style.css"/>
            <link rel="stylesheet" href="/bouygues/static/src/scss/website.ui.scss"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
<!--            <script type="text/javascript" src="/bouygues/static/src/js/variant_mixin.js"></script>-->
            <script type="text/javascript" src="/bouygues/static/src/js/website_sale.js"></script>
        </xpath>
    </template>
    <template id="bouygues_assets_backend" name="bouygues assets" inherit_id="web.assets_backend">
       <xpath expr="." position="inside">
           <script type="text/javascript" src="/bouygues/static/src/js/add_to_board_menu.js"></script>
           <script type="text/javascript" src="/bouygues/static/src/js/rental_configurator_controller.js"></script>
       </xpath>
        <xpath expr="." position="inside">
            <link rel="stylesheet" href="/bouygues/static/src/scss/sticky_headers.scss"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/bouygues/static/src/js/widgets/switch_company_menu.js"></script>
        </xpath>
    </template>
</odoo>
