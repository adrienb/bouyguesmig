<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="stock_warning_tree_view" model="ir.ui.view">
            <field name="name">stock.warning.tree</field>
            <field name="model">stock.warning</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>
        <record id="stock_warning_form_view" model="ir.ui.view">
            <field name="name">stock.warning.form</field>
            <field name="model">stock.warning</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <div class="oe_title">
                            <h1>
                                <field name="name"/>
                            </h1>
                        </div>
                        <group>
                            <field name="filename" invisible="1"/>
                            <field name="file" filename="filename"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="action_stock_warning" model="ir.actions.act_window">
            <field name="name">Stock Warning</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">stock.warning</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="bouygues.stock_warning_tree_view"/>
        </record>
        <menuitem action="action_stock_warning" id="menu_action_stock_warning" parent="stock.menu_stock_config_settings" sequence="20"/>
</odoo>