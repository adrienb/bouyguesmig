<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="cron_sync_saml_data" model="ir.cron">
        <field name="name">Provide SAML fields on users</field>
        <field name="model_id" ref="model_res_users" />
        <field name="state">code</field>
        <field name="code">model.cron_sync_saml_data(ids=[], exclude=False, limit=False)</field>
        <field name='interval_number'>1</field>
        <field name='interval_type'>days</field>
        <field name="numbercall">-1</field>
        <field name="nextcall" eval="(DateTime.now().replace(hour=1)).strftime('%Y-%m-%d %H:%M:%S')" />
        <field name="doall" eval="False" />
        <field name="active" eval="False" />
    </record>
</odoo>
